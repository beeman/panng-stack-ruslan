{
  "npmScope": "panng",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "api": {
      "tags": []
    },
    "api-core-data-access": {
      "tags": ["scope:api", "type:data-access"]
    },
    "api-core-feature": {
      "tags": ["scope:api", "type:feature"]
    },
    "api-listing-data-access": {
      "tags": ["scope:api", "type:data-access"]
    },
    "api-listing-feature": {
      "tags": ["scope:api", "type:feature"]
    }
  }
}
